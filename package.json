{
    "name": "todoer",
    "displayName": "TODOer",
    "description": "YUP YUP YUP",
    "version": "0.0.1",
    "publisher": "AlexArthurs",
    "engines": {
        "vscode": "^1.28.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.JiraSetup",
        "onCommand:extension.CreateJiraToken",
        "onCommand:extension.CreateJiraIssue",
        "onCommand:extension.CreateTrelloCard",
        "onCommand:extension.GetTrelloToken",
        "onCommand:extension.EnterTrelloToken"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.JiraSetup",
                "title": "TODOer: Jira Setup"
            },
            {
                "command": "extension.CreateJiraToken",
                "title": "TODOer: Create Jira Token"
            },
            {
                "command": "extension.CreateJiraIssue",
                "title": "TODOer: Create Jira Issue"
            },
            {
                "command": "extension.CreateTrelloCard",
                "title": "TODOer: Create Trello Card"
            },
            {
                "command": "extension.GetTrelloToken",
                "title": "TODOer: Get Trello Token"
            },
            {
                "command": "extension.EnterTrelloToken",
                "title": "TODOer: Enter Trello Token"
            }
        ],
        "configuration": {
            "title": "TODOer Configuration",
            "properties": {
                "TODOer.trelloToken": {
                    "type": "string",
                    "description": "Your Trello Token",
                    "default": ""
                },
                "TODOer.jiraHost": {
                    "type": "string",
                    "description": "Your Jira Host",
                    "default": ""
                },
                "TODOer.jiraClientToken": {
                    "type": "string",
                    "description": "Your Jira Client Token",
                    "default": ""
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "base-64": "^0.1.0",
        "jira-connector": "^2.9.0",
        "open": "0.0.5"
    }
}
